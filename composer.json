{
    "name": "mackstar/spout",
    "description":"A BEAR.Sunday based CMS",
    "license": "MIT",
    "keywords":[
        "PHP",
        "CMS"
    ],
    "authors":[
        {
            "name": "Richard McIntyre"
        }
    ],
    "require":{
        "bear/package":"dev-develop",
        "danielstjules/stringy": "1.*",
        "rhumsaa/uuid":"2.4",
        "robmorgan/phinx": "*",
        "imagine/imagine": "v0.5.0",
        "zendframework/zend-validator": "*",
        "aptoma/twig-markdown": "0.2.*",
        "michelf/php-markdown": "~1.3"
    },
    "require-dev":{
        "phpspec/prophecy": "1.2.*@dev",
        "bear/dev-package":"dev-master"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload":{
        "psr-4":{
            "Mackstar\\Spout\\": "src/",
            "Mackstar\\Spout\\Test\\": "tests/src/"
        }
    },
    "bin": ["bin/spout"]
}
